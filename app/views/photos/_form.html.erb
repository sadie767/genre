<%= render "layouts/errors", :object => @photo %>

<%= form_for @photo do |f| %>
  <%= f.label :url %>
  <br>
  <%= f.text_field :url %>
  <br><br>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br><br>
  <%= f.label :description %>
  <br>
  <%= f.text_area :description, size: "40x5" %>
  <br><br>
  <%= f.submit %>
<% end %>
